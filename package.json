{
  "name": "amplify-base",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test": "npm run test:unit",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "validate:schema": "ajv compile -s src/DataMock/daily_leads_schema.json --spec draft7 --strict --all-errors -c ajv-formats",
    "validate:mocks": "node scripts/validate-no-mocks.mjs",
    "seed:templates": "node scripts/seed-templates.mjs",
    "generate:examples": "tsx scripts/generateExamples.ts",
    "validate:examples": "ajv validate -s src/DataMock/daily_leads_schema.json -d src/DataMock/daily_leads_examples.json --spec draft7 --all-errors -c ajv-formats",
    "generate:types": "tsx scripts/generateTypes.ts"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^4.2.30",
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/client-scheduler": "^3.840.0",
    "@aws-sdk/client-ses": "^3.840.0",
    "@aws-sdk/client-ssm": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@heroicons/vue": "^2.2.0",
    "aws-amplify": "^6.15.4",
    "docx": "^9.5.1",
    "dompurify": "^3.2.6",
    "dotenv": "^16.5.0",
    "jszip": "^3.10.1",
    "lambda-stream": "^0.6.0",
    "marked": "^16.1.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@itseasy21/mcp-knowledge-graph": "^1.0.7",
    "@tsconfig/node22": "^22.0.0",
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/jszip": "^3.4.0",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "ajv": "^8.17.1",
    "ajv-cli": "^0.6.0",
    "ajv-formats": "^2.1.1",
    "autoprefixer": "^10.4.17",
    "aws-cdk": "^2.1000.2",
    "aws-cdk-lib": "^2.204.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "json5": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.4.35",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.12"
  }
}
